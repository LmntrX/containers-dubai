{
  "name": "containersdubai",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "^3.0.1"
  },
  "homepage": "http://containersdubai.com",
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build",
